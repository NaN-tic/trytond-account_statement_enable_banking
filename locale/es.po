msgctxt "model:ir.message,text:msg_error_create_session"
msgid "Error creating the session: \"%(error_code)s\" \"%(error_message)s\""
msgstr ""
"Se ha producido un error mientras se intentaba crear la sesión: "
"\"%(error_code)s\" \"%(error_message)s\""

